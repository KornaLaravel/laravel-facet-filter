{
    "name": "mgussekloo/laravel-facet-filter",
    "description": "Simple facet filtering in Laravel projects, hassle free.",
    "keywords": [
        "facet",
        "filter",
        "search",
        "navigation",
        "laravel",
        "faceted search",
        "smart filter"
    ],
    "autoload": {
        "psr-4": {
            "Mgussekloo\\FacetFilter\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Martijn Gussekloo",
            "email": "1426964+mgussekloo@users.noreply.github.com"
        }
    ],
    "require": {
    	"php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mgussekloo\\FacetFilter\\FacetFilterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.6",
        "orchestra/testbench": "^8.0",
        "nunomaduro/larastan": "^2.0"
    }
}
